{
  "extends": "airbnb-base"
  ,
    "env": {
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "no-console": "off",
      "eol-last": ["error", "never"]
    }
}